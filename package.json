{
  "private": true,
  "name": "rodinia-report",
  "scripts": {
    "dev": "concurrently \"npm:remix:dev\" \"npm:css:dev\" -c \"bgRed,bgBlue\"",
    "dev:railway": "railway run concurrently \"npm:remix:dev\" \"npm:css:dev\" -c \"bgRed,bgBlue\"",
    "remix:dev": "npx remix run",
    "remix:build": "npx remix build",
    "css:dev": "postcss styles --base styles --dir app/styles -w",
    "css:build": "postcss styles --base styles --dir app/styles --env production",
    "build": "npm run css:build && npm run remix:build",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "@remix-run/node": "0.17.0",
    "@remix-run/react": "0.17.0",
    "@remix-run/serve": "0.17.0",
    "@sentry/node": "^6.4.0",
    "@sentry/react": "^6.4.0",
    "@sentry/tracing": "^6.4.0",
    "firebase": "^8.5.0",
    "firebase-admin": "^9.8.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.0",
    "remix": "0.17.0"
  },
  "devDependencies": {
    "@remix-run/dev": "0.17.0",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.1.0",
    "cssnano": "^5.0.2",
    "postcss": "^8.2.15",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": "14"
  },
  "sideEffects": false
}
